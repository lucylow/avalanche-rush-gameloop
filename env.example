# Avalanche Rush Reactive Quest Environment Variables

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Avalanche API Key for contract verification
AVALANCHE_API_KEY=your_avalanche_api_key_here

# Reactive Network Configuration
REACTIVE_NETWORK_ADDRESS=0x0000000000000000000000000000000000000000
REACTIVE_SUBSCRIPTION_ID=1

# NFT Metadata Configuration
NFT_BASE_URI=https://api.avalanche-rush.com/metadata/
NFT_NAME=Avalanche Rush Achievements
NFT_SYMBOL=AVAXACH

# Network URLs (for reference)
AVALANCHE_RPC_URL=https://api.avax.network/ext/bc/C/rpc
AVALANCHE_FUJI_RPC_URL=https://api.avax-test.network/ext/bc/C/rpc
REACTIVE_RPC_URL=https://rpc.reactive-network.com
LASNA_RPC_URL=https://rpc.lasna.reactive-network.com

# Gas Reporting
REPORT_GAS=true
